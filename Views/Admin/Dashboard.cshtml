@model IEnumerable<Registation>
@{
    ViewData["Title"] = "Dashboard";
}

<h1>Dashboard</h1>
<div>
    <ul>
    @{ 
        foreach(Registation reg in Model)
        {
           <li> @reg.Email</li>
          
        }
    }
        </ul>
    @*<div>
        @Model.CandidateName
    </div>
    <div>
        @Model.Email
    </div>*@
    <a class="btn btn-primary form-control" asp-area="" asp-controller="Admin" asp-action="AddCenter">Add Center</a>
    <a  class="btn btn-success form-control" asp-controller="Admin" asp-action="GenrateAdmitCard">Genrate Admit Card</a>

    <table class="table table-striped">
        <thead>
            <tr>
                <th>Firstname</th>
                <th>Lastname</th>
                <th>Email</th>
                <th>View/Approve</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>John</td>
                <td>Doe</td>
                <td>john@example.com</td>
                <td><a class="btn btn-primary" asp-area="" asp-controller="Admin" asp-action="EnrollApprove">View</a></td>
            </tr>
        </tbody>
    </table>
</div>

